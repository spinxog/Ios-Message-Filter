    "import pandas as pd\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.feature_extraction.text import CountVectorizer\n",
    "from sklearn.naive_bayes import MultinomialNB\n",
    "from sklearn.pipeline import make_pipeline\n",
    "from sklearn.metrics import accuracy_score\n",
    "import sqlite3\n",
    "import os\n",
    "import re\n",
    "\n",
    "# Loading dataset\n",
    "data = pd.read_csv('spam.csv')\n",
    "\n",
    "# Split into training and testing sets\n",
    "X_train, X_test, y_train, y_test = train_test_split(data['Message'], data['Category'], test_size=0.2, random_state=42)\n",
    "\n",
    "# Naive bayes pipeline\n",
    "model = make_pipeline(CountVectorizer(), MultinomialNB())\n",
    "\n",
    "# Model Training\n",
    "model.fit(X_train, y_train)\n",
    "\n",
    "# Model testing\n",
    "y_pred = model.predict(X_test)\n",
    "print(f\"Accuracy: {accuracy_score(y_test, y_pred)}\")\n",
    "\n",
    "# Imgessage text import\n",
    "# Path to the iMessage database\n",
    "db_path = os.path.expanduser('~/Library/Messages/chat.db')\n",
    "\n",
    "# Database\n",
    "conn = sqlite3.connect(db_path)\n",
    "\n",
    "# Fetch Messages\n",
    "query = '''\n",
    "SELECT message.text, handle.id as sender\n",
    "FROM message\n",
    "JOIN handle ON message.handle_id = handle.ROWID\n",
    "WHERE message.text IS NOT NULL\n",
    "'''\n",
    "messages = pd.read_sql_query(query, conn)\n",
    "\n",
    "# Close connection\n",
    "conn.close()\n",
    "\n",
    "# Defines saved contact\n",
    "saved_contacts = [\n",
    "    \"19173376550\",
    "    \"applejuice@gmail.com\",
    "]\n",
    "\n",
    "# Most contacts\n",
    "def normalize_phone_number(phone):\n",
    "    phone = re.sub(r'[^0-9]', '', phone)\n",
    "    if phone.startswith('1') and len(phone) == 11:\n",
    "        phone = phone[1:]  ,
    "    return phone\n",
    "\n",
    "# Normalizes emails\n",
    "def normalize_email(email):\n",
    "    return email.lower().strip()\n",
    "\n",
    "# Ignores saved contacts\n",
    "saved_contacts = [\n",
    "    normalize_phone_number(contact) if '@' not in contact else normalize_email(contact)\n",
    "    for contact in saved_contacts\n",
    "]\n",
    "\n",
    "messages['sender'] = messages['sender'].apply(\n",
    "    lambda x: normalize_phone_number(x) if '@' not in x else normalize_email(x)\n",
    ")\n",
    "\n",
    "# Debugging check\n",
    "print(\"Normalized Saved Contacts:\", saved_contacts)\n",
    "print(\"Unique Senders in Messages:\", messages['sender'].unique())\n",
    "\n",
    "# Ignores filter application from saved contacts\n",
    "# Only classify messages from unknown senders\n",
    "unknown_messages = messages[~messages['sender'].isin(saved_contacts)]\n",
    "\n",
    "# Spam prediction for unknown contacts\n",
    "unknown_messages['prediction'] = model.predict(unknown_messages['text'])\n",
    "\n",
    "# Result\n",
    "messages['prediction'] = 'ham'  # Default to \"ham\"\n",
    "messages.loc[unknown_messages.index, 'prediction'] = unknown_messages['prediction']\n",
    "\n",
    "# Filters spam messages\n",
    "spam_messages = messages[messages['prediction'] == 'spam']\n",
    "print(\"Spam messages from unknown senders:\")\n",
    "print(spam_messages)\n",
    "\n",
    "spam_messages.to_csv('filtered_spam_messages.csv', index=False)"
   ]
